{
  "name": "Skeletor Syntax",
  "author": "Ram√≥n M. Cros, ported by dieghernan",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#2B2836",
        "foreground": "#DCE7FD"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "meta.doctype",
        "entity.name.tag.doctype.html",
        "meta.tag.metadata.doctype",
        "meta.tag.metadata.doctype.html"
      ],
      "settings": {
        "foreground": "#655E7F"
      }
    },
    {
      "name": "Strings, constants and properties",
      "scope": [
        "string",
        "constant.other",
        "constant.language",
        "support.type.property-name",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "Booleans and Numbers",
      "scope": [
        "constant.language.boolean",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FF8ADB"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F36A66"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#F3E4A2"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "Attribute names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Operators, tags, at-rules and built-ins",
      "scope": [
        "keyword.operator",
        "entity.name.tag",
        "keyword.control.at-rule",
        "meta.at-rule",
        "storage.type.tag.verbatim.yaml"
      ],
      "settings": {
        "foreground": "#F3E4A2"
      }
    },
    {
      "name": "Selectors and functions",
      "scope": [
        "entity.name.tag.css",
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Keywords and classes",
      "scope": [
        "keyword",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Regular expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Markdown - Heading Symbols",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#DCE7FD"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Raw code",
      "scope": [
        "markup.raw.block.markdown",
        "markup.inline.raw.string.markdown",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link",
        "markdown",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "Markdown - Link Text",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#F3E4A2"
      }
    },
    {
      "name": "Markdown - Lists Bullets",
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "Javascript - Keyword",
      "scope": [
        "storage.type.js",
        "keyword.control.js",
        "constant.language.null.js",
        "constant.language.undefined.js",
        "keyword.control.conditional.js",
        "keyword.control.flow.js",
        "keyword.control.trycatch.js"
      ],
      "settings": {
        "foreground": "#F3E4A2"
      }
    },
    {
      "name": "Javascript - Class Name",
      "scope": [
        "entity.name.type.instance.js",
        "support.class.js"
      ],
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "SCSS - Strings",
      "scope": [
        "string.scss",
        "string.quoted.double.scss",
        "string.quoted.single.scss"
      ],
      "settings": {
        "foreground": "#DCE7FD"
      }
    },
    {
      "name": "SCSS - Keywords",
      "scope": [
        "keyword.control.scss",
        "meta.at-rule.scss",
        "keyword.scss",
        "keyword.control.for.scss",
        "meta.at-rule.for.scss",
        "keyword.control.operator"
      ],
      "settings": {
        "foreground": "#F3E4A2"
      }
    },
    {
      "name": "SCSS - Variables",
      "scope": [
        "variable.scss",
        "variable.interpolation.scss"
      ],
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "PHP - Keywords",
      "scope": [
        "storage.type.php",
        "storage.type.function.php",
        "variable.language.this.php",
        "storage.modifier.extends.php"
      ],
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "PHP - Constants",
      "scope": "constant.language.php",
      "settings": {
        "foreground": "#FF8ADB"
      }
    },
    {
      "name": "PHP - Properties",
      "scope": "variable.other.property.php",
      "settings": {
        "foreground": "#93B4FF"
      }
    },
    {
      "name": "PHP - Delimiters",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#7B94A5"
      }
    },
    {
      "name": "YAML - Directive",
      "scope": [
        "meta.directives.yaml",
        "keyword.other.directive.yaml",
        "constant.numeric.yaml-version.yaml"
      ],
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "YAML - Keywords and Importants",
      "scope": [
        "variable.other.anchor.yaml",
        "keyword.control.flow.anchor.yaml",
        "keyword.control.flow.alias.yaml"
      ],
      "settings": {
        "foreground": "#84FBA2"
      }
    },
    {
      "name": "YAML - At rules",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "YAML - Strings",
      "scope": "string.unquoted.plain.out.yaml",
      "settings": {
        "foreground": "#DCE7FD"
      }
    }
  ],
  "colors": {
